{
	"name": "@sikaeducation/ui",
	"author": "Kyle Coberly <kyle.coberly@gmail.com>",
	"version": "0.13.1",
	"repository": "git@github.com:sikaeducation/janus.git",
	"license": "Apache 2.0",
	"source": "src/index.ts",
	"type": "module",
	"exports": {
		".": "./dist/index.js",
		"./components.css": "./dist/index.css",
		"./reset": "./dist/styles/_reset.scss",
		"./styles": "./dist/styles/_index.scss",
		"./types": "./dist/index.d.ts"
	},
	"module": "./dist/index.js",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/*"
	],
	"scripts": {
		"start": "storybook dev -p 6006",
		"lint": "eslint --ignore-path .gitignore --fix .",
		"build": "rm -rf dist && microbundle --globals react/jsx-runtime=jsx --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react -f modern,cjs && npm run build:static-files",
		"build:storybook": "storybook build",
		"build:static-files": "cp -r src/styles dist && cp -r src/styles/fonts dist/fonts",
		"test": "test-storybook -- --detectOpenHandles",
		"test:watch": "test-storybook --watch",
		"test:ci": "concurrently -k -s first -n \"Storybook,Component Tests\" -c \"bgGray.white,auto\" \"npm:ci:serve\" \"npm:ci:wait-to-test\"",
		"ci:wait-to-test": "wait-on tcp:127.0.0.1:6006 && test-storybook --url http://127.0.0.1:6006",
		"ci:serve": "http-server storybook-static --port 6006 --silent",
		"_generate-dependency-version": "cat package-lock.json | jq '.dependencies' | sha1sum > .dependency-version",
		"dependencies": "npm run _generate-dependency-version"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.1.1",
		"@fortawesome/free-solid-svg-icons": "^6.1.1",
		"@fortawesome/react-fontawesome": "^0.1.18",
