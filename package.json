{
  "name": "janus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "test": "npm run test:unit && npm run test:components && npm run test:features",
    "test:ci": "npm run test:unit && npm run test:components -- --url 'storybook-server:6006' && npm run test:features",
    "test:unit": "react-scripts test --watchAll=false --transformIgnorePatterns \"node_modules/(?!react-markdown)/\"",
    "test:unit:watch": "react-scripts test --transformIgnorePatterns \"node_modules/(?!react-markdown)/\"",
    "test:components": "test-storybook",
    "test:components:watch": "test-storybook --watch",
    "test:features": "npx playwright test features/",
    "test:features:watch": "nodemon -L -x 'npm run test:features' -e ts"
  },
