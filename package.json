{
	"name": "@sikaeducation/ui",
	"author": "Kyle Coberly <kyle.coberly@gmail.com>",
	"version": "0.7.46",
	"repository": "git@github.com:sikaeducation/janus.git",
	"license": "Apache 2.0",
	"source": "src/index.ts",
	"exports": {
		"./types": "./dist/index.d.ts",
		"./components.css": "./dist/index.css",
		"./styles": "./dist/styles/_index.scss",
		"./reset": "./dist/styles/_reset.scss",
		".": "./dist/index.js"
	},
	"module": "./dist/index.js",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/*"
	],
	"scripts": {
		"start": "start-storybook -p 6006",
		"lint": "eslint --fix ./src",
		"build": "rm -rf dist && microbundle --globals react/jsx-runtime=jsx --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react -f modern,cjs && npm run build:static-files",
		"build:storybook": "build-storybook",
		"build:static-files": "cp -r src/styles dist && cp -r src/styles/fonts dist/fonts",
		"test": "test-storybook",
		"test:watch": "test-storybook --watch",
		"test:ci": "concurrently -k -s first -n \"Storybook,Component Tests\" -c \"bgGray.white,auto\" \"npm:ci:serve\" \"npm:ci:wait-to-test\"",
		"ci:wait-to-test": "wait-on tcp:127.0.0.1:6006 && test-storybook --url http://127.0.0.1:6006",
		"ci:serve": "http-server storybook-static --port 6006 --silent",
		"_generate-dependency-version": "cat package-lock.json | jq '.dependencies' | sha1sum > .dependency-version",
		"dependencies": "npm run _generate-dependency-version"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.1.1",
		"@fortawesome/free-solid-svg-icons": "^6.1.1",
		"@fortawesome/react-fontawesome": "^0.1.18",
		"@material-ui/core": "^4.12.4",
		"@material-ui/lab": "^4.0.0-alpha.61",
		"eslint": "^8.47.0",
		"prettier": "^3.0.2",
		"react-gravatar": "^2.6.3",
		"react-markdown": "^7.1.2",
		"react-redux": "^8.0.5",
		"react-router-dom": "^6.2.1",
		"react-syntax-highlighter": "^15.4.5",
		"react-use-clipboard": "^1.0.7",
		"remark-frontmatter": "^4.0.1",
		"remark-gfm": "^3.0.1",
		"remark-unwrap-images": "^3.0.1"
	},
	"peerDependencies": {
		"classnames": "^2.3.1",
		"date-fns": "^2.28.0",
		"lodash": "^4.17.21",
		"prop-types": "^15.8.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@babel/plugin-transform-modules-commonjs": "^7.16.7",
		"@playwright/test": "^1.37.1",
		"@sikaeducation/eslint-config-ts-react": "^0.3.2",
		"@storybook/addon-a11y": "^6.5.14",
		"@storybook/addon-actions": "^6.5.14",
		"@storybook/addon-essentials": "^6.5.14",
		"@storybook/addon-interactions": "^6.5.14",
		"@storybook/addon-links": "^6.5.14",
		"@storybook/builder-webpack5": "^6.5.14",
		"@storybook/jest": "^0.0.10",
		"@storybook/manager-webpack5": "^6.5.14",
		"@storybook/node-logger": "^6.5.14",
		"@storybook/preset-create-react-app": "^4.1.2",
		"@storybook/react": "^6.5.14",
		"@storybook/test-runner": "^0.9.4",
		"@storybook/testing-library": "^0.0.13",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^12.1.2",
		"@testing-library/react-hooks": "^7.0.2",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.0.3",
		"@types/lodash": "^4.14.178",
		"@types/node": "^16.11.26",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"@types/react-gravatar": "^2.6.10",
		"@types/react-syntax-highlighter": "^13.5.2",
