		"@types/d3": "^7.4.0",
		"classnames": "^2.3.1",
		"date-fns": "^2.28.0",
		"lodash": "^4.17.21",
		"microbundle": "^0.15.1",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^17.0.2",
		"react-gravatar": "^2.6.3",
		"react-markdown": "^8.0.7",
		"react-redux": "^8.0.5",
		"react-router-dom": "^6.2.1",
		"react-scripts": "5.0.1",
		"react-syntax-highlighter": "^15.4.5",
		"react-use-clipboard": "^1.0.7",
		"remark-frontmatter": "^4.0.1",
		"remark-unwrap-images": "^3.0.1",
		"sass": "^1.45.2",
